{
  "version": 2,
  "name": "uniden-chatbot",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "chatbot-app/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/chatbot-app/build/(.*)",
      "dest": "/chatbot-app/build/$1"
    }
  ]
}
